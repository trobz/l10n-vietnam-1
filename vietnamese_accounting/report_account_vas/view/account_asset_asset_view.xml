<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- Invoice Supplier -->
        <record id="view_account_asset_asset_form_inherit" model="ir.ui.view">
            <field name="name">account.asset.asset.form.inherit</field>
            <field name="model">account.asset.asset</field>
            <field name="inherit_id" ref="account_asset.view_account_asset_asset_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@string='General']">
                    <group string="Asset Card Information" col="4">
                        <field name="delivery_number"/>
                        <field name="delivery_date"/>
                        <field name="basic_specification"/>
                        <field name="manufacturer" widget="selection"/>
                        <field name="manufacture_year"/>
                        <field name="department"/>
                        <field name="start_date_in_use"/>
                        <field name="capacity"/>
                        <field name="date_of_liquidation"/>
                        <field name="reason"/>
                    </group>
                </xpath>
                <field name="code" position="after">
                    <field name="reference" />
                </field>

            </field>
        </record>
    </data>
</openerp>
